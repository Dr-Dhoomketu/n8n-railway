{
  "build": {
    "builder": "DOCKERFILE",
    "dockerfilePath": "Dockerfile"
  },
  "deploy": {
    "restartPolicyType": "ALWAYS",
    "healthcheckPath": "/healthz"
  },
  "volumes": [
    {
      "mountPath": "/data",
      "name": "n8n-data"
    }
  ],
  "env": {
    "N8N_PORT": "5678",
    "N8N_HOST": "0.0.0.0",
    "N8N_USER_FOLDER": "/data",
    "WEBHOOK_URL": "https://${RAILWAY_STATIC_URL}"
  }
}
